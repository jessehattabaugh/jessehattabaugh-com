	],
	"license": "Artistic-2.0",
	"name": "jessehattabaugh-com",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"clean": "rimraf shots/*.diff.png shots/*.curr.png && cd home && npm run clean",
		"deploy": "now --public && now alias",
		"dev": "npm run clean && npm install && concurrently \"cd home && npm run watch\" \"npm run serve\"",
		"e2e": "lab --timeout 100000",
		"format": "prettier --write **/*.{html,js,json,css,md,webmanifest}",
		"kill": "killall -9 node && killall -9 npm",
		"lab": "lab",
		"lighthouse": "lighthouse-ci http://localhost:1234 --score=75",
		"now": "now",
		"preinstall": "cd home && npm install && cd ../save && npm install",
		"serve": "kill-port 1234 && serve --no-clipboard -l 1234 home/public",
		"stage": "now --public",
		"start": "cd home && npm run now-build && cd .. && npm run serve",
		"test": "start-test 1234 e2e",
		"test-all": "npm run e2e && npm run lighthouse"
	},
	"version": "0.0.3"
}

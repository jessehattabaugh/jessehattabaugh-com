{
	"author": "Jesse Hattabaugh",
	"browserslist": "> 1%",
	"bugs": {
		"url": "https://github.com/jessehattabaugh/jessehattabaugh-com/issues"
	},
	"description": "Personal website of Jesse Hattabaugh",
	"devDependencies": {
		"chokidar-cli": "^2.1.0",
		"concurrently": "^5.0.2",
		"kill-port": "^1.6.0",
		"lighthouse-ci": "^1.10.0",
		"pug-cli": "^1.0.0-alpha6",
		"rimraf": "^3.0.0",
		"serve": "^11.3.0",
		"start-server-and-test": "^1.10.6"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
		"Hattabaugh"
	],
	"license": "UNLICENSED",
	"name": "jessehattabaugh-com",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"audit": "lighthouse-ci http://localhost:5000 --score=60",
		"build": "pug pages --pretty --out public",
		"serve": "serve public",
		"start": "concurrently \"npm run watch\" \"npm run serve\"",
		"stop": "kill-port 5000",
		"test": "start-test 5000 audit",
		"watch": "npm run build && chokidar pages/*.pug -c \"npm run build\""
	},
	"version": "0.0.0"
}

	"description": "my website",
	"devDependencies": {
		"@hapi/lab": "^19.1.0",
		"code": "^5.2.4",
		"concurrently": "^4.1.1",
		"fs-extra": "^8.1.0",
		"husky": "^3.0.0",
		"kill-port": "^1.5.1",
		"lighthouse-ci": "^1.9.0",
		"looks-same": "^7.2.1",
		"node-mocks-http": "^1.7.6",
		"now": "^15.7.0",
		"now-env": "^3.2.0",
		"prettier": "^1.18.2",
		"puppeteer": "^1.18.1",
		"rimraf": "^2.6.3",
		"serve": "^11.0.2",
		"start-server-and-test": "^1.9.1"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
		"Hattabaugh"
	],
	"license": "Artistic-2.0",
	"name": "jessehattabaugh-com",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"clean": "rimraf static/dist static/.cache shots/*.diff.png shots/*.curr.png **/package-lock.json",
		"deploy": "now --public && now alias",
		"dev": "npm run clean && npm install && concurrently \"cd static && npm run watch\" \"npm run serve\"",
		"e2e": "lab --timeout 100000",
		"format": "prettier --write **/*.{html,js,json,css,md,webmanifest}",
		"kill": "killall -9 node && killall -9 npm",
		"lab": "lab",
		"lighthouse": "lighthouse-ci http://localhost:1234 --score=80",
		"now": "now",
		"preinstall": "cd static && npm install && cd ../save && npm install",
		"serve": "kill-port 1234 && serve --no-clipboard -l 1234 static/dist",
		"stage": "now --public",
		"start": "cd static && npm run now-build && cd .. && npm run serve",
		"test": "start-test 1234 test-all",
		"test-all": "npm run e2e && npm run lighthouse"
	},
	"version": "0.0.3"
}

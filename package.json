{
	"author": "Jesse Hattabaugh",
	"bugs": {
		"url": "https://github.com/jessehattabaugh/jessehattabaugh-com/issues"
	},
	"description": "Personal website of Jesse Hattabaugh",
	"devDependencies": {
		"cross-env": "^6.0.3",
		"kill-port": "^1.6.0",
		"lighthouse-ci": "^1.10.0",
		"parcel": "latest",
		"posthtml-extend": "^0.3.0",
		"rimraf": "^3.0.0",
		"start-server-and-test": "^1.10.6"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
		"Hattabaugh"
	],
	"license": "UNLICENSED",
	"name": "jessehattabaugh-com",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"audit": "lighthouse-ci http://localhost:1234/index.html --score=60",
		"clean": "rimraf .cache dist",
		"build": "npm run clean && parcel build src/*.html",
		"start": "npm run clean && parcel serve src/*.html",
		"stop": "kill-port 1234",
		"test": "cross-env DEBUG=start-server-and-test start-test http://localhost:1234/index.html audit"
	},
	"version": "0.0.0"
}

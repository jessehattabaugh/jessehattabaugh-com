	"bugs": {
		"url": "https://github.com/jessehattabaugh/jessehattabaugh-com/issues"
	},
	"description": "my website",
	"devDependencies": {
		"code": "^5.2.0",
		"concurrently": "^4.0.1",
		"fs-extra": "^7.0.0",
		"husky": "^0.14.3",
		"kill-port": "^1.3.1",
		"lab": "^16.0.0",
		"looks-same": "^3.3.0",
		"now": "^11.4.1",
		"parcel": "^1.9.7",
		"prettier": "^1.14.2",
		"puppeteer": "^1.7.0",
		"rimraf": "^2.6.2",
		"serve": "^10.0.0",
		"start-server-and-test": "^1.7.0"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
		"Hattabaugh"
	],
	"license": "Artistic-2.0",
	"name": "jessehattabaugh-com",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"build": "npm run clean && parcel build src/index.html",
		"clean": "rimraf dist .cache shots/*.diff.png shots/*.curr.png",
		"deploy": "npm run build && now && now alias",
		"dev": "npm run clean && concurrently \"parcel watch src/index.html\" \"npm run serve\"",
		"e2e": "lab --timeout 100000",
		"format": "prettier --write ./*",
		"kill": "killall -9 node && killall -9 npm",
		"precommit": "npm run format",
		"prepush": "npm test",
		"serve": "kill-port 1234 && serve --no-clipboard -l 1234 dist",
		"start": "npm run build && npm run serve",
		"test": "start-test 1234 e2e"
	},
	"version": "0.0.2"
}

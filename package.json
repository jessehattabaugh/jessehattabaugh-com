	"description": "my website",
	"devDependencies": {
		"ansi-colors": "^3.0.5",
		"code": "^5.2.0",
		"fs-extra": "^7.0.0",
		"husky": "^0.14.3",
		"kill-port": "^1.3.1",
		"lab": "^16.0.0",
		"lighthouse": "^3.0.3",
		"looks-same": "^3.3.0",
		"node": "^10.9.0",
		"now": "^11.4.0",
		"npm": "^6.4.0",
		"parcel": "^1.9.7",
		"prettier": "^1.14.2",
		"puppeteer": "^1.7.0",
		"serve": "^10.0.0",
		"start-server-and-test": "^1.7.0"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
		"Hattabaugh"
	],
	"license": "Artistic-2.0",
	"name": "jessehattabaugh-com",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"build": "npm run clean && parcel build src/index.html",
		"clean": "rm -rf dist && rm -rf .cache",
		"deploy": "npm run build && now --static --public dist",
		"dev": "npm run clean && parcel watch src/index.html & npm run serve",
		"format": "prettier --write ./*",
		"kill": "killall -9 node && killall -9 npm",
		"precommit": "npm format",
		"prepush": "npm test",
		"test": "start-test 1234 e2e",
		"start": "npm run build && npm run serve",
		"serve": "kill-port 1234 && serve --no-clipboard -l 1234 dist",
		"e2e": "lab --timeout 100000"
	},
	"version": "0.0.2"
}

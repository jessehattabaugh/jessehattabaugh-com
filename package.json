	"description": "my website",
	"devDependencies": {
		"code": "^5.2.4",
		"concurrently": "^4.1.0",
		"fs-extra": "^7.0.1",
		"husky": "^1.2.0",
		"kill-port": "^1.3.2",
		"lab": "^18.0.0",
		"looks-same": "^4.0.0",
		"now": "^12.1.5",
		"parcel-bundler": "^1.10.3",
		"posthtml-extend": "^0.3.0",
		"prettier": "^1.15.2",
		"puppeteer": "^1.10.0",
		"rimraf": "^2.6.2",
		"serve": "^10.1.1",
		"start-server-and-test": "^1.7.10"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
		"Hattabaugh"
	],
	"license": "Artistic-2.0",
	"name": "jessehattabaugh-com",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"clean": "rimraf dist .cache shots/*.diff.png shots/*.curr.png",
		"deploy": "now --public && now alias",
		"dev": "npm run clean && concurrently \"parcel watch src/*.html\" \"npm run serve\"",
		"e2e": "lab --timeout 100000",
		"format": "prettier --write **/*.{html,js,json,css,md,webmanifest}",
		"kill": "killall -9 node && killall -9 npm",
		"now-build": "npm run clean && parcel build src/*.html",
		"serve": "kill-port 1234 && serve --no-clipboard -l 1234 dist",
		"start": "npm run now-build && npm run serve",
		"test": "start-test 1234 e2e"
	},
	"version": "0.0.2"
}

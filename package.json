{
	"author": "Jesse Hattabaugh",
	"bugs": {
		"url": "https://github.com/jessehattabaugh/jessehattabaugh-com/issues"
	},
	"description": "Personal website of Jesse Hattabaugh",
	"devDependencies": {
		"cross-env": "^6.0.3",
		"kill-port": "^1.6.0",
		"lighthouse-ci": "^1.10.0",
		"parcel": "latest",
		"posthtml-extend": "^0.3.0",
		"rimraf": "^3.0.0",
		"serve": "^11.3.0",
		"start-server-and-test": "^1.10.6",
		"workbox-cli": "^4.3.1"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
		"Hattabaugh"
	],
	"license": "UNLICENSED",
	"name": "jessehattabaugh-com",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jessehattabaugh/jessehattabaugh-com.git"
	},
	"scripts": {
		"audit": "lighthouse-ci http://localhost:5000 --score=90",
		"build": "npm run clean && parcel build src/*.html --no-content-hash --no-source-maps --no-cache && npm run workbox",
		"clean": "rimraf .cache dist",
		"dev": "npm run build && parcel serve src/*.html --port=5000 --open --no-hmr --no-cache",
		"start": "npm run build && serve dist",
		"stop": "kill-port 5000",
		"test": "start-test 5000 audit",
		"testDebug": "cross-env DEBUG=start-server-and-test && npm t",
		"workbox": "workbox injectManifest workbox-config.js"
	},
	"version": "0.0.0"
}

		"fs-extra": "^7.0.1",
		"husky": "^1.3.1",
		"kill-port": "^1.3.2",
		"lab": "^18.0.2",
		"lighthouse-ci": "^1.4.1",
		"looks-same": "^7.2.0",
		"node-mocks-http": "^1.7.3",
		"now": "^14.2.3",
		"now-env": "^3.1.0",
		"prettier": "^1.16.4",
		"puppeteer": "^1.13.0",
		"rimraf": "^2.6.3",
		"serve": "^10.1.2",
		"start-server-and-test": "^1.7.12"
	},
	"homepage": "https://github.com/jessehattabaugh/jessehattabaugh-com#readme",
	"keywords": [
		"Jesse",
